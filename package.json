{
  "name": "typescript-react-sass-starter",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react-scripts-ts": "^2.14.0",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar -q src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar -q src/ -o src/ --watch --recursive",
    "start": "HTTPS=false npm-run-all -p watch-css start-ts",
    "build": "npm run build-css && react-scripts-ts build",
    "start-ts": "react-scripts-ts start",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  }
}
